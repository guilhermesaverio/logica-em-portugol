algoritmo "SOMA"
var
   v1, v2, s: inteiro
funcao soma(x, y: inteiro) : inteiro
inicio

   retorne(x + y)

fimfuncao

inicio
      escreval("Digite um numero: ")
      leia(v1)
      escreval("Digite o segundo numero: ")
      leia(v2)
      
      s <- soma(v1,v2)
      
      escreval("Resultado: ",s)
      



fimalgoritmo